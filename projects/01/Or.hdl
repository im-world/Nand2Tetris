 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    //invert-or
    Not(in = a, out = aBar);
    Not(in = b, out = bBar);
    Nand(a = aBar, b = bBar, out = out);
}
